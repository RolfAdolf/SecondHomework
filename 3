y = [0:0.1:3*pi];
[u, v] = meshgrid(x, y);
a = 1;
b = 1;
X = (a+b.*cos(v)).*cos(u);
Y = (a+b.*cos(v)).*sin(u);
Z = b.*sin(v);
figure;
surfc(X, Y, Z);
view(-38, 18);

