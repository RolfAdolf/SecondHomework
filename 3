a)
x = [-5:0.1:5];
y = [-5:0.1:5];
[X, Y] = meshgrid(x, y);
a = 3;
Z = a.*X.*exp(-(X.^2) - (Y.^2));
figure;
surfc(X, Y, Z);
view(-38, 18);


