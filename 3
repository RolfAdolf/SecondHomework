c)
x = [0:0.1:(2.5)*pi];
y = [0:0.1:4];
[bet, r] = meshgrid(x, y);
a = 2;
X = r.*cos(x);
Y = r.*sin(x);
Z = (r.^2)/(2*a);
figure;
surfc(X, Y, Z);
view(-38, 18);

