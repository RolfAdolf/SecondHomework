m)
x = [-2*pi:0.1:2*pi];
y = [-3:0.1:3];
[u, v] = meshgrid(x, y);
a = 2;
R = 3;
X = (1+v.*cos((0.5).*u)).*cos(u);
Y = (1+v.*cos((1/2).*u)).*sin(u);
Z = v.*sin((1/2).*u);
figure;
surfc(X, Y, Z);
view(-38, 18);

