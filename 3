l)x = [0:0.1:5];
y = [0:0.1:2*pi];
[v, phi] = meshgrid(x, y);
a = 2;
b = 2;
R = 3;
X = (a+R.*sin(phi)).*cos(v);
Y = (a+R.*sin(phi)).*sin(v);
Z = R.*cos(phi) + b.*v;
figure;
surfc(X, Y, Z);
view(-38, 18);

