d)
x = [0:0.1:2*pi];
y = [-pi:0.1:pi];
[phi, bet] = meshgrid(x, y);
a = 2;
b = 3;
X = a.*(phi).*((exp(bet) + exp(-bet))./2);
Y = b.*(phi).*((exp(bet) - exp(-bet))./2);
Z = phi.^2;
figure;
surfc(X, Y, Z);
view(-38, 18);

