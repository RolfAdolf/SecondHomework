k)
x = [0:0.1:(3)];
y = [-2*(pi):0.1:2*(pi)];
[r, u] = meshgrid(x, y);
a = 1;
um = 0.2;
w = 4;
phi = (pi/2);
X = r.*cos(u);
Y = r.*sin(u);
Z = a.*exp(-um*r).*sin(w.*r+phi);
figure;
surfc(X, Y, Z);
view(-38, 18);

